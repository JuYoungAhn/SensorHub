extends template
block content
  body(onload='getHash()')
  div(class='navigater')
    h1 Main Page
    div
      a(href='/enertalk') Enertalk Login
    div
      a(href='/netatmo') Netatmo Login
  div(class='enertalk dashboard')
    if session.enertalk_access_token != null
      include enertalk_dashboard
  div(class='netatmo dashboard')
    if session.netatmo_access_token != null
      include netatmo_dashboard
